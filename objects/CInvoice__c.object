    <validationRules xmlns="http://soap.sforce.com/2006/04/metadata">
        <fullName>Check_amount</fullName>
        <active>true</active>
        <errorConditionFormula>Amount_Without_Tax__c &gt;  Total_Amount__c</errorConditionFormula>
        <errorDisplayField>Amount_Without_Tax__c</errorDisplayField>
        <errorMessage>The amount without tax cannot be greater than the total amount. Check that first and try again.</errorMessage>
    </validationRules>
    <validationRules xmlns="http://soap.sforce.com/2006/04/metadata">
        <fullName>Check_total_tax</fullName>
        <active>true</active>
        <errorConditionFormula>(Amount_Without_Tax__c + Taxation__c) &lt;&gt; Total_Amount__c</errorConditionFormula>
        <errorDisplayField>Amount_Without_Tax__c</errorDisplayField>
        <errorMessage>The total amount between the amount without taxation and the actual taxation must be equal to the expense total amount!</errorMessage>
    </validationRules>
    <validationRules xmlns="http://soap.sforce.com/2006/04/metadata">
        <fullName>No_further_than_expense_report_date</fullName>
        <active>true</active>
        <description>Dates from invoices must be less than the expense report.</description>
        <errorConditionFormula>Date__c &gt;  CExpense__r.Date__c</errorConditionFormula>
        <errorDisplayField>Date__c</errorDisplayField>
        <errorMessage>The date from the invoice must not be greater than the expense report&apos;s date.</errorMessage>
    </validationRules>
</CustomObject>
