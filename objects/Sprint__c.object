    <validationRules>
        <fullName>Avoid_Product_Backlog_renaming</fullName>
        <active>true</active>
        <description>Avoid users to rename the sprint Product Backlog.</description>
        <errorConditionFormula>AND(
ISCHANGED(Name),
PRIORVALUE(Name)= &quot;Product Backlog&quot;
)</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>It isn&apos;t possible to change the name &quot;Product Backlog&quot;.</errorMessage>
    </validationRules>
