        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Number_of_completed_Story_Points__c</fullName>
        <externalId>false</externalId>
        <label>Number of completed Story Points</label>
        <summaryFilterItems>
            <field>Work_Item__c.Status__c</field>
            <operation>equals</operation>
            <value>Done</value>
        </summaryFilterItems>
        <summaryForeignKey>Work_Item__c.Sprint__c</summaryForeignKey>
        <summaryOperation>count</summaryOperation>
        <trackTrending>false</trackTrending>
        <type>Summary</type>
    </fields>
    <fields>
        <fullName>Number_of_not_completed_Story_Points__c</fullName>
